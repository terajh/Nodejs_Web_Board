extends layout


block content
  div(class="container")
    table(class="table")
      thead 
        tr
          th Number
          th Board List
          th Made by
      tbody
        -for(var i=0;i<title.length;i++)
          tr
            td #{i+1}
            td: a(href="../data/#{title[i].title}") #{title[i].title}
            td #{title[i].name}
  hr  



















  
      

